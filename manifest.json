{
  "name": "Hide Unnecessary Page",
  "description" : "Hide Unnecessary Page From Search Results.",
  "version": "1.0",

  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search?*"],
      "js": ["jquery-3.3.1.min.js", "main.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "trashBoxIn.png",
    "trashBoxOut.png",
    "trashBox.png"
  ]
}
